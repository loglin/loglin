{
  "name": "@loglin/reporter-console",
  "version": "1.1.1",
  "license": "MIT",
  "author": "Marvin Rudolph",
  "description": "Console reporter for loglin",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loglin/loglin.git",
    "directory": "packages/reporter-console"
  },
  "bugs": {
    "url": "https://github.com/loglin/loglin/issues"
  },
  "homepage": "https://github.com/loglin/loglin/tree/main",
  "main": "./dist/reporter-console.js",
  "module": "./dist/reporter-console.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/reporter-console.mjs",
      "require": "./dist/reporter-console.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts --format cjs,esm --dts",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "loglin": "^1.1.1"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "keywords": [
    "logger",
    "fancy",
    "reporter",
    "console"
  ]
}
